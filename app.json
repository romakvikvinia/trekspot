{
  "expo": {
    "name": "Trekspot.io",
    "slug": "comtrekspot",
    "version": "1.0.2",
    "newArchEnabled": "true",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": false,
      "bundleIdentifier": "com.trekspot.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.923726965679-tap0vlets0dvclh7uertjalc0f1ueq50"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.trekspot.app"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0208e3d2-29fc-4d09-9a72-11fae25997f6"
      }
    },
    "owner": "trekspot",
    "expo": {
      "plugins": [
        "expo-apple-authentication",
        "@react-native-google-signin/google-signin",
        [
          "expo-splash-screen",
          {
            "backgroundColor": "#fff",
            "image": "./assets/splash-icon.png",
            "dark": {
              "image": "./assets/splash-icon-dark.png",
              "backgroundColor": "#000000"
            },
            "imageWidth": 200
          }
        ],
        {
          "iosUrlScheme": "com.googleusercontent.apps.923726965679-tap0vlets0dvclh7uertjalc0f1ueq50"
        },
        [
          "expo-location",
          {
            "locationAlwaysAndWhenInUsePermission": "Allow Trekspot to use your location."
          }
        ],
        [
          "expo-document-picker",
          {
            "iCloudContainerEnvironment": "Production"
          }
        ],
        [
          "expo-image-picker",
          {
            "photosPermission": "Allow Trekspot to access your photos."
          }
        ],
        [
          "expo-media-library",
          {
            "photosPermission": "Allow Trekspot to access your photos.",
            "savePhotosPermission": "Allow Trekspot to save photos.",
            "isAccessMediaLocationEnabled": true
          }
        ],
        "@sentry/react-native/expo",
        {
          "organization": "trekspot",
          "project": "Trekspot",
          "url": "https://sentry.io/"
        }, 
      ]
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0208e3d2-29fc-4d09-9a72-11fae25997f6"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      "expo-localization",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "trekspot"
        }
      ]
    ]
  }
}
